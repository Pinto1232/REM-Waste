{
  "name": "rem-waste",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "gc": "bash ./scripts/create-component.sh",
    "gc:ui": "bash ./scripts/create-component.sh --ui",
    "remove-comments": "node ./scripts/remove-comments.js",
    "format-components": "node ./scripts/format-components.js",
    "format:prettier": "prettier --write \"src/components/**/*.{ts,tsx,js,jsx}\"",
    "format:eslint": "eslint \"src/components/**/*.{ts,tsx,js,jsx}\" --fix",
    "format:all": "npm run format-components && npm run format:prettier && npm run format:eslint",
    "lint:components": "eslint \"src/components/**/*.{ts,tsx,js,jsx}\"",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.29.19",
    "axios": "^1.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "10.4.14",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "postcss": "8.4.24",
    "prettier": "^2.8.8",
    "tailwindcss": "3.3.2",
    "typescript": "^5.0.4",
    "vite": "^4.3.9"
  }
}
